<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.Net/schema/beanmapping.xsd">

    <configuration>

        <stop-on-errors>false</stop-on-errors>

        <date-format>yyyy-MM-dd'T'HH:mm:ssX</date-format>
        <!--<date-format>yyyy-MM-dd HH:mm:ss</date-format>-->

        <wildcard>true</wildcard>

    </configuration>

    <mapping>
        <class-a>com.chinacaring.hmsrmyy.dao.entity.Appointment</class-a>
        <class-b>com.chinacaring.hmsrmyy.dto.his.request.register.RegisterRequestHis</class-b>
        <field>
            <a>patientName</a>
            <b>realName</b>
        </field>
        <field>
            <a>idCard</a>
            <b>icno</b>
        </field>
        <field>
            <a>medicareType</a>
            <b>medicalType</b>
        </field>
        <field>
            <a>deptName</a>
            <b>departmentName</b>
        </field>
        <field>
            <a>doctorCode</a>
            <b>emplCode</b>
        </field>
        <field>
            <a>patientCode</a>
            <b>patientNo</b>
        </field>
        <field>
            <a>appointmentId</a>
            <b>bookingNo</b>
        </field>
        <field>
            <a>payChannel</a>
            <b>payMode</b>
        </field>
        <field>
            <a>orderNo</a>
            <b>transNo</b>
        </field>
    </mapping>

    <mapping>
        <class-a>com.chinacaring.hmsrmyy.dto.front.request.ClinicRecordRequest</class-a>
        <class-b>com.chinacaring.hmsrmyy.dto.his.request.clinicRecords.ClinicRecordsRequestHis</class-b>
        <field>
            <a>patientCode</a>
            <b>patientNO</b>
        </field>
    </mapping>

    <mapping>
        <class-a>com.chinacaring.hmsrmyy.dto.front.response.ClinicRecordResponse</class-a>
        <class-b>com.chinacaring.hmsrmyy.dto.his.response.clinicRecords.ItemType</class-b>
        <field>
            <a>registerId</a>
            <b>regNO</b>
        </field>
        <field>
            <a>doctorCode</a>
            <b>emplCode</b>
        </field>
        <field>
            <a>doctorName</a>
            <b>docName</b>
        </field>
    </mapping>

    <mapping>
        <class-a>com.chinacaring.hmsrmyy.dto.front.response.OutpatientMedicalRecordsResponse</class-a>
        <class-b>com.chinacaring.hmsrmyy.dto.his.response.outPatientMedicalRecords.ItemType</class-b>
        <field>
            <a>registerId</a>
            <b>regNo</b>
        </field>
        <field>
            <a>patientCode</a>
            <b>patientNo</b>
        </field>
        <field>
            <a>doctorCode</a>
            <b>emplCode</b>
        </field>
        <field>
            <a>doctorName</a>
            <b>docName</b>
        </field>
    </mapping>
</mappings>